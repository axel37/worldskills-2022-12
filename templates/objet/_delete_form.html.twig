<form method="post" action="{{ path('app_objet_delete', {'reference': objet.reference}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet objet ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objet.reference) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
